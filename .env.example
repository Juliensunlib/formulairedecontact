# Supabase Configuration
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_SUPABASE_URL=your_supabase_url

# Typeform Configuration
# Créez un token sur https://admin.typeform.com/account#/section/tokens
# avec la permission "Read responses"
VITE_TYPEFORM_TOKEN=your_typeform_token_here
VITE_TYPEFORM_FORM_ID=your_form_id_here

# Airtable Configuration
# Créez un token sur https://airtable.com/create/tokens
# avec les scopes: data.records:read et data.records:write
VITE_AIRTABLE_TOKEN=your_airtable_token_here
VITE_AIRTABLE_BASE_ID=your_base_id_here
VITE_AIRTABLE_TABLE_NAME=Leads Solaires
# Table de destination pour les données Typeform
VITE_AIRTABLE_TYPEFORM_TABLE_NAME=Formulaire de contact
